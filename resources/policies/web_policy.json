{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "ec2:Describe*",
        "autoscaling:Describe*"
      ],
      "Resource": [
        "*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:*"
      ],
      "Resource": [
        "arn:aws:s3:::AWS-ACCOUNT-CLUSTER-NAME-data",
        "arn:aws:s3:::AWS-ACCOUNT-CLUSTER-NAME-data/*"
        "arn:aws:s3:::AWS-ACCOUNT-CLUSTER-NAME-log",
        "arn:aws:s3:::AWS-ACCOUNT-CLUSTER-NAME-log/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:List*",
        "s3:Get*"
      ],
      "Resource": [
        "arn:aws:s3:::AWS-ACCOUNT-CLUSTER-NAME-cloudinit/web",
        "arn:aws:s3:::AWS-ACCOUNT-CLUSTER-NAME-cloudinit/web/*",
        "arn:aws:s3:::AWS-ACCOUNT-CLUSTER-NAME-config",
        "arn:aws:s3:::AWS-ACCOUNT-CLUSTER-NAME-config/*"
      ]
    }
  ]
}